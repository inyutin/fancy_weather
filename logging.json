{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "standard": {
      "style": "{",
      "format": "{asctime} {name} {levelname:1.1} - {message}"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "level": "DEBUG",
      "formatter": "standard"
    }
  },
  "loggers": {
    "": {
      "level": "DEBUG",
      "handlers": [
        "console"
      ]
    }
  }
}
